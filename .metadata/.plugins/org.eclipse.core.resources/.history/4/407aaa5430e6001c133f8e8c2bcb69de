package com.greenshop.dto;


public class CategoryDTO {
	private long id;
	private String name;
	private ImageDTO imageId;
	
	public CategoryDTO() {
		// TODO Auto-generated constructor stub
	}

	
	public CategoryDTO(long id, String name, ImageDTO imageId) {
		super();
		this.id = id;
		this.name = name;
		this.imageId = imageId;
	}


	public long getId() {
		return id;
	}

	public void setId(long id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public ImageDTO getImage() {
		return imageId;
	}

	public void setImage(ImageDTO image) {
		this.imageId = image;
	}
	
}
